  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^7.2.3",
    "nodemon": "^1.14.7",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^4.1.1",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-for": "^2.1.1",
    "postcss-mixins": "^6.2.0"
  }
}
